{
	"grammars": [
		{
			"name": "flamingo",
			"camelcase": "Flamingo",
			"scope": "source.fl",
			"path": ".",
			"file-types": ["fl"],
			"highlights": ["queries/highlights.scm"],
			"first-line-regex": "#!.*\\bflamingo$"
		}
	],
	"metadata": {
		"version": "1.0.0",
		"license": "ASL",
		"description": "Flamingo Tree-sitter",
		"authors": [
			{
				"name": "Aymeric Wibo"
			}
		],
		"links": {
			"repository": "https://github.com/tree-sitter/tree-sitter-flamingo"
		}
	},
	"bindings": {
		"c": true,
		"go": true,
		"node": true,
		"python": true,
		"rust": true,
		"swift": true
	}
}
